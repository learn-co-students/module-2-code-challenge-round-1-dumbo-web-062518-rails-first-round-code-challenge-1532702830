<h1>Create a new Heroine!</h1>
<h3><%= @heroine.super_name %> <%= @heroine.errors.messages[:name][0] %></h3>
<%= form_for @heroine do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :super_name %>
<%= f.text_field :super_name %><br>
<%= f.collection_select :power_id, Power.all, :id, :name %><br>
<%= f.submit %>
<% end %>
