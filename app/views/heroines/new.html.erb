<h1>Create New Heroine</h1>

<ul>
  <% @heroine.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
</ul>

<%= form_for @heroine do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :super_name %><br>
  <%= f.text_field :super_name %><br>
  <%= f.collection_select :power_id, Power.all, :id, :name %><br>
  <%= f.submit %><br>
<% end %>
